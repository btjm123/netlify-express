<html>
    <head>
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.9/semantic.min.css"/>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.9/semantic.min.js"></script>
        <link href="/css/index.css" rel="stylesheet" type="text/css">
    </head>
</html>

<body>
    <div class='main-container'>
        <div class="ui cards">
            <% for (var i=0;i<data.length;i++)  { %>
                <div class="card">
                  <div class="content">
                    <div class="header">Created by <%= data[i].owner.login %></div>
                    <div class="description">
                      on <%= dates[i] %>
                    </div>
                    <div class="description">
                        <% if (data[i].description == null || data[i].description == "")  { %>
                            No details!
                        <% } else { %>
                            Description: <%= data[i].description %>
                        <% } %>
                      </div>
                  </div>
                  <% 
                    var randomNum = Math.floor(Math.random() * 10);
                    
                  %>
                  <a href=<%= data[i].html_url %> target='_blank' style='color:white;'>
                    <div class="ui bottom red attached button" style='color:white;'>
                        <i class="add icon"></i>
                        Check it out!
                    </a>
                  </div>
                </div>
            <% } %>
        </div>
    </div>
    <script
  src="https://code.jquery.com/jquery-3.5.1.min.js"
  integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
  crossorigin="anonymous"></script>
</body>



